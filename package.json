{
  "name": "dazedbear.github.io",
  "version": "4.36.0",
  "homepage": "https://github.com/dazedbear/dazedbear.github.io",
  "repository": {
    "type": "git",
    "url": "git@github.com:dazedbear/dazedbear.github.io.git"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "test:e2e:page": "PLAYWRIGHT_HTML_REPORT=e2e-page-report npx playwright test --project page-test-desktop page-test-mobile --config=tests/e2e/playwright.config.ts",
    "test:e2e:api": "PLAYWRIGHT_HTML_REPORT=e2e-api-report npx playwright test --project api-test --config=tests/e2e/playwright.config.ts",
    "test:e2e:failsafe": "PLAYWRIGHT_HTML_REPORT=e2e-failsafe-report npx playwright test --project failsafe-test-desktop failsafe-test-mobile --config=tests/e2e/playwright.config.ts",
    "test:unit": "TZ=UTC jest --coverage --coverageDirectory=${COVERAGE_DIR:=artifacts/coverage} --config=jest.config.js",
    "lint:eslint": "next lint --fix",
    "lint:stylelint": "stylelint src/styles/*.css --fix",
    "build": "CACHE_CLIENT_ENABLED=false next build",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\"",
    "prepare": "is-ci || husky install",
    "task:failsafe": "node scripts/failsafe.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "revert": true,
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "build",
              "release": "minor"
            },
            {
              "type": "refactor",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": "Bug Fixes",
                "hidden": false
              },
              {
                "type": "docs",
                "section": "Documentation",
                "hidden": false
              },
              {
                "type": "style",
                "section": "Others",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": "Others",
                "hidden": false
              },
              {
                "type": "patch",
                "section": "Hotfix",
                "hidden": false
              },
              {
                "type": "perf",
                "section": "Performance",
                "hidden": false
              },
              {
                "type": "test",
                "section": "Testing",
                "hidden": false
              },
              {
                "type": "build",
                "section": "Build",
                "hidden": false
              },
              {
                "type": "ci",
                "section": "CI/CD",
                "hidden": false
              },
              {
                "type": "chore",
                "section": "Others",
                "hidden": false
              },
              {
                "type": "revert",
                "section": "Others",
                "hidden": false
              }
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "@docsearch/react": "^3.3.2",
    "@reduxjs/toolkit": "^1.5.1",
    "@vercel/kv": "^0.2.3",
    "ajv": "^8.6.0",
    "ajv-formats": "^2.1.0",
    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.10.5",
    "env-var": "^7.1.1",
    "http-errors": "^2.0.0",
    "logrocket": "^1.0.14",
    "lqip-modern": "^2.0.0",
    "next": "^13.5.4",
    "next-redux-cookie-wrapper": "^2.0.1",
    "next-redux-wrapper": "^7.0.2",
    "node-fetch": "^3.2.0",
    "notion-client": "^6.13.4",
    "notion-utils": "^6.13.4",
    "nprogress": "^0.2.0",
    "object-hash": "^2.1.1",
    "p-all": "^4.0.0",
    "p-map": "^5.3.0",
    "qs": "^6.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-notion-x": "^6.13.4",
    "react-redux": "^7.2.3",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.0.5",
    "sitemap": "^7.0.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@next/bundle-analyzer": "^12.0.10",
    "@playwright/test": "^1.25.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/http-errors": "^1.8.2",
    "@types/ioredis": "^4.28.5",
    "@types/jest": "^28.1.7",
    "@types/node": "^17.0.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^16.9.17",
    "@types/react-redux": "^7.1.21",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "^13.5.4",
    "fast-xml-parser": "^4.4.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "notion-types": "^3.1.1",
    "postcss": "^8.4.31",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "semantic-release": "^19.0.3",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^27.0.0",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^28.0.8",
    "typescript": "^4.3.2",
    "webpack": "^5.76.0"
  }
}
